  .user {
    position: relative;
    display: inline-block;
    margin-right: 10px;
  }

  .user .usertext {
    visibility: hidden;
    width: 215px;
    background-color: rgb(255, 255, 255);
    color: rgb(0, 0, 0);
    text-align: center;
    border-radius: 6px;
    border-color: #ada5a5;
    position: absolute;
    z-index: 1;
    top: 125%;
    left: 50%;
    margin-left: -150px;
    opacity: 0;
    transition: opacity 0.3s;
    padding: 6px 10px;
    box-shadow: 0px 0px 5px 0px grey;
  }

  .user .usertext::after {
    content: "";
    position: absolute;
    top: 100%;
    left: 50%;
    margin-left: -5px;
    border-width: 5px;

  }

  .user:hover .usertext {
    visibility: visible;
    opacity: 1;
  }

  #lillylogo {
    height: 39px;
    margin-bottom: -45px;
    margin-left: 7px;
  }


  /* Modal Content */
  .modal-content-email {
    background-color: #fefefe;
    width: 50%;
    margin: auto;
    padding: 20px;
    border: 1px solid #888;
    width: 80%;
  }

  /* The Close Button */
  .close-support {
    color: #aaaaaa;
    float: right;
    font-size: 28px;
    font-weight: bold;
  }

  .close-support:hover,
  .close-support:focus {
    color: #000;
    text-decoration: none;
    cursor: pointer;
  }

  #dropdown-menu-help-button>li>a:hover {
    background-color: white !important;
    background-image: none !important;
  }

  span.dropdown-help-button.open:before {
    position: fixed;
    content: "";
    background-color: rgba(0, 0, 0, 0.4);
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 0;
  }
