    <style>
      .execution_task_land {
  position: relative;
  display: inline-block;
}

.execution_task_land .tooltiptext_land {
  visibility: hidden;
  width: 250px;
  background-color: white;
  color: #000000;
  text-align: left;
  border-radius: 6px;
  padding: 5px 5px;

  /* Position the tooltip */
  position: absolute;
  z-index: 1;
  line-height: 18px;
}

.execution_task_land:hover .tooltiptext_land {
  visibility: visible;
  margin-top: 20px;
}
        body {
            height: 100vh;
        }

        section {
            height: calc(100% - 36px);
        }

        @media (min-height: 1080px) {
            section {
                height: calc(100% - 64px);
            }
        }

        section .main-left {
            height: 100%;
            padding: 9% 6%;
            background-image: url(/img/lilly_logo_bg.png);
            background-repeat: no-repeat;
            background-position: bottom;
            background-position-x: left;
        }

        section .main-left h1 {
            font: normal normal bold 24px/28px Arial;
            color: #d52b1e;
            margin-bottom: 2%;
        }

        section .main-left p {
            font: normal normal normal 16px/18px Arial;
            color: #000;
            margin-bottom: 8%;
        }

        .TA_div {
            position: absolute;
            top: -77px;
            left: -125px;
            width: 247px;
            height: 250px;
            transform: rotate(45deg);
            background: #d52b1e;
        }

        .TA_div span {
            position: relative;
            top: 110px;
            width: 200px;
            left: 110px;
            text-align: center;
            font-size: 16px;
            font-family: arial;
            display: block;
            transform: rotate(0 deg) !important;
            transform: rotate(315deg);
            color: white;
            word-break: break-all;
        }



        .btn-home {
            cursor: pointer;
            background: #d52b1e 0% 0% no-repeat padding-box;
            box-shadow: 0px 0px 5px #00000071;
            border-radius: 3px;
            border: none;
            padding: 2% 3%;
            color: white;
            margin-bottom: 5px;
        }

        .btn-home-catalog {
            cursor: pointer;
            background: white 0% 0% no-repeat padding-box;
            box-shadow: 0px 0px 5px #00000071;
            border-radius: 3px;
            padding: 2% 3%;
            color: #d52b1e;
            border-color: #d52b1e;
            border: 1px solid;
            margin-bottom: 5px;
        }

        .continue_btn {
            width: 100px;
            height: 50px;
            border-radius: 4px;
            box-shadow: 4px 4px 8px 0 rgba(0, 0, 0, 0.1);
            background-color: #d52b1e;
            font-family: Arial;
            font-size: 16px;
            font-weight: bold;
            color: #fff;
            margin-right: 10px;
        }

        .cancel_btn {
            width: 100px;
            height: 50px;
            text-align: center;
            white-space: nowrap;
            vertical-align: middle;
            text-decoration-color: black;
            /* margin: 10px 41px;*/
            border: none;
            font-size: 16px;
            font-weight: 500;
        }

        .backg-img {
            height: 100%;
            background-image: url(/img/right_bg.png);
            background-repeat: no-repeat;
            background-size: cover;
        }

        .new-flag {
            position: absolute;
            background: #FFFFFF 0% 0% no-repeat padding-box;
            box-shadow: 0px 0px 6px #00000029;
            border-radius: 8px;
            padding: 0 1%;
            color: #d52b1e;
            font-size: 12px !important;
            margin-top: -4%;
            margin-left: -5%;
        }

        footer {
            /* background: #F4F4F4 0% 0% no-repeat padding-box; */
            padding: 1% 3%;
            width: 100%;
        }

        footer span,
        .btn-infor {
            font: normal normal normal 14px/16px Arial !important;
        }

        .landing-page-cet {
            overflow: hidden;
        }

        .smalldiv {
            text-align: right !important;
            display: block;
        }

        .TA_div span {
            top: 100px;
            left: 104px;
        }

